{
  "name": "sb-types",
  "version": "0.2.2-beta",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "sb-types": "./dist/index.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "rm -rf ./dist && ENV=prod tsup",
    "build:dev": "rm -rf ./dist && ENV=dev tsup",
    "type-check": "tsc -p tsconfig.json --pretty --noEmit"
  },
  "author": {
    "name": "Gerald Andrei Tamasi",
    "email": "info@tamasigerald.com",
    "url": "https://tamasigerald.com/"
  },
  "license": "ISC",
  "description": "CLI tool for generating typescript types from Storyblok datasources",
  "repository": {
    "type": "git",
    "url": "https://github.com/tamasigerald/sb-types"
  },
  "keywords": [
    "storyblok",
    "storyblok-types",
    "datasources",
    "typescript",
    "cli",
    "types",
    "types-generator"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "clear": "^0.1.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5",
    "figlet": "^1.7.0",
    "mkdirp": "^3.0.1",
    "prettier": "^3.2.5"
  },
  "devDependencies": {
    "@types/clear": "^0.1.4",
    "@types/figlet": "^1.5.8",
    "@types/node": "^20.12.11",
    "esbuild": "0.21.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
